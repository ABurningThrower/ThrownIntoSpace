[network]

limits/debugger/max_chars_per_second=16384
limits/debugger/max_queued_messages=1024
limits/debugger/max_errors_per_second=100
limits/debugger/max_warnings_per_second=100

[rendering]

renderer/rendering_method="mobile"
driver/depth_prepass/enable=false
environment/defaults/default_clear_color=Color(0.0352941, 0.0352941, 0.0352941, 1)
